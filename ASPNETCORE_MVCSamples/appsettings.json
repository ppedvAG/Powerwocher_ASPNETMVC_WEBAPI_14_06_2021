{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Position": {
    "Title": "ASPNET Core Kurs",
    "Name": "Joe Smith"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Debug"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31
        }
      }
    ]
  },
  "ConnectionStrings": {
    "ASPNETCORE_MVCSamplesContextConnection": "Server=(localdb)\\mssqllocaldb;Database=ASPNETCORE_MVCSamples;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}